<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>SHACL Validator for OSLO2</display-name>
  <servlet>
  	<servlet-name>HomeServlet</servlet-name>
  	<display-name>HomeServlet</display-name>
  	<description></description>
  	<servlet-class>validator.OSLO2.HomeServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>validateServlet</servlet-name>
  	<display-name>validateServlet</display-name>
  	<description></description>
  	<servlet-class>validator.OSLO2.validateServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>test</servlet-name>
  	<display-name>test</display-name>
  	<description></description>
  	<servlet-class>validator.OSLO2.test</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>HomeServlet</servlet-name>
  	<url-pattern>/home</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>validateServlet</servlet-name>
  	<url-pattern>/validate</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>test</servlet-name>
  	<url-pattern>/test</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
 	<welcome-file>index.jsp</welcome-file>     
  </welcome-file-list>
</web-app>
